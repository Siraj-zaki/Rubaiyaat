    "start": "serve -s build",
    "build": "react-scripts  build",
    "heroku-postbuild": "npm run build",
    "dev": "react-scripts   start",
    "lint:fix": "eslint — ext --fix --ext .js,.jsx .",
    "lint": "eslint — ext .js,.jsx,.ts,.tsx src — color — fix",
    "ESLINT": "eslint  --fix"
  },
  "plugins": [
    "unused-imports"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.28.0"
  }
}
